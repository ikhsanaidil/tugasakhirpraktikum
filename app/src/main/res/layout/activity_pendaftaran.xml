<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pendaftaran">

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:src="@drawable/pendaftaran"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/editTextTextNamalengkap"
      android:layout_width="378dp"
      android:layout_height="48dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="nama lengkap"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.727"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.144"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/editTextTexttempatLahir"
      android:layout_width="371dp"
      android:layout_height="50dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="tempat lahir"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.6"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.309"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/editTextTextTanggalLahir"
      android:layout_width="378dp"
      android:layout_height="48dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="tanggal lahir"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.636"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.227"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/editTextTextNamaOrangtua"
      android:layout_width="378dp"
      android:layout_height="48dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="Nama Orangtua"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.636"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.399"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/editTextTextKelas"
      android:layout_width="378dp"
      android:layout_height="48dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="Kelas"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.636"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.484"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <EditText
      android:id="@+id/editTextTpaAsal"
      android:layout_width="378dp"
      android:layout_height="48dp"
      android:background="@drawable/text"
      android:ems="20"
      android:hint="Tpa asal"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.727"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.575"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints" />

  <Button
      android:id="@+id/masuk"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:backgroundTint="#20D003"
      android:text="Masuk"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.752"
      app:strokeColor="@color/green" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="130dp"
      android:layout_height="27dp"
      android:text="Pendaftaran"
      android:textColor="#070707"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.063"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

  <ImageView
      android:id="@+id/kembali"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:src="@drawable/kembali"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.134"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.053"
      app:strokeColor="@color/green"
      tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>
